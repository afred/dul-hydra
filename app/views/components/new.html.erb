<h1>New Component</h1>

<%= form_tag({:controller => "components", :action => "create"}, :multipart => true) do %>
  <%= label_tag(:contentfile, "Content File") %>
  <%= file_field_tag(:contentfile) %>
  <br />
  <%= render "shared/apo_selector" %>
  <br />
  <%= submit_tag "Create Component" %>
<% end %>
