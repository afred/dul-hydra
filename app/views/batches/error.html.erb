<%= content_tag :h1, I18n.t('batch.web.page_titles.batch', :id => @batch.id) %>
<% unless @errors.empty? %>
	<%= content_tag :h3, I18n.t('batch.web.headings.object_validation_errors') %>
	<%= content_tag :ul do %>
		<% @errors.each do |error| %>
			<%= content_tag :li, error %>	
		<% end %>
	<% end %>
<% end %>
