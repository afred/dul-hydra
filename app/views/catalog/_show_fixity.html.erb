<div class="well">
  <ul class="nav nav-list">
	<li class="nav-header">
	  Fixity
	</li>
	<% if @document.last_fixity_check_on %>
	<li>
	  <table class="table table-condensed">
		<tr>
		  <th scope="row">Checked:</th>
		  <td><%= format_date @document.last_fixity_check_on %></td>
		</tr>
		<tr>
		  <th scope="row">Outcome:</th>
		  <td><%= render_last_fixity_check_outcome %></td>
		</tr>
	  </table>
	</li>
	<% else %>
	<li>
	  No fixity checks
	</li>
	<% end %>
  </ul>
</div>
