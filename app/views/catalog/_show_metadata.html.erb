<div class="navbar">
  <div class="navbar-inner">
	<ul class="nav">
	  <li>
		<%= link_to "Download XML", metadata_path(@document.id, format: 'xml') %>
	  </li>
	  <li>
		<%= link_to "Fcrepo View", fcrepo_admin.object_datastream_path(@document.id, "descMetadata") %>
	  </li>
	</ul>
  </div>
</div>
<table class="table">
  <% metadata_fields.each do |field| %>
    <% metadata_field_values(field).each do |value| %>
	<tr>
	  <th scope="row"><%= metadata_field_label field %></th>
	  <td><%= value %></td>
	</tr>
	<% end %>
  <% end %>
</table>
