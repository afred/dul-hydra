<%# This is now deprecated in favor of displaying children on a separate page and will likely be removed %>

<% if document[:content_metadata_parsed_s] %>
  <% content_metadata = document.parsed_content_metadata %>
  <% content_metadata.each do |structure| %>
    <%= display_structure(structure).html_safe %>
  <% end %>
<% else %>
  <ul>
    <% document.children.each do |child| %>
      <% child_doc = SolrDocument.new(child) %>
      <li><%= title_display_solr_value(child_doc) %></li>
    <% end %>
  </ul>
<% end %>

