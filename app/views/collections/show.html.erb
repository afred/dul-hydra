<h1>Collection <%= @collection.has_title? ? @collection.title.first : @collection.pid %></h1>

<%= link_to " Collection List", collections_path %>

<%= render :partial => 'dul_hydra/show_summary', :locals => { :obj => @collection } %>

<h2>Members</h2>

<ul>
<% @collection.items.each do |i| %>
  <li><%= link_to i.pid, item_path(i) %></li>
<% end %>
</ul>
